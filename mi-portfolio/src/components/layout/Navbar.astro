---
import styles from './../../styles/navbar.module.css';

/* c√≥digo javascript dentro del frontmatter (entre ---) */
const links = [
    {href: "/", label: "Inicio"},
    {href: "/proyectos", label: "Proyectos"},
    {href: "/contacto", label: "Contacto"}
];
--- 

<nav class={styles.navStyles}>
    <div class={styles.navChild}>
        <h2 id={styles.myNameHome}>Diego <span class={styles.lastNameStyles}>Marchandon</span></h2>
    </div>
    <div class={styles.navChild}>
      <ul class={styles.ulStyles}>
      {links.map(link => (
          <li class={styles.liStyles}>
            <a class={styles.aStyles} href={link.href}>{link.label}</a>
          </li>
        ))}
      </ul>
  </div>
</nav>